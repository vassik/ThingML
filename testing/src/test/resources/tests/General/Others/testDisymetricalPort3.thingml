import "../../thingml.thingml"

thing TestDisymetricalPort3 includes Test
@test "12 # I"
{
	message m1();
	message m2();
	message m3();

	provided port pin {
		sends m2
	}
	required port pout {
		sends m1, m2
	}
	statechart Chart init Init {
		internal event Test?Done
		action TestDone()
		
		state Init {
			on entry TestOut('\'I\'')

			internal event e : Test?In
			guard e.c == '\'1\''	
			action pout!m1()	

			internal event e : Test?In
			guard e.c == '\'2\''		
			action pout!m2()
		}
	}
}

configuration conf {
	instance test : TestDisymetricalPort3
	
	connector test.pout => test.pin
}