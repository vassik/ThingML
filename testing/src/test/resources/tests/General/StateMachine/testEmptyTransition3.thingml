import "../../thingml.thingml"

thing TestEmptyTransition3 includes Test 
@test "t # IJKLMM"
{
	statechart TestEmptyTransition init I {
		state I {
			on entry TestOut('\'I\'')

			transition -> J
		}
		state J {
			on entry TestOut('\'J\'')
			
			transition -> M
			event m : Test?In
			guard m.c == '\'t\''

			transition -> K
		}
		state K {
			on entry TestOut('\'K\'')
			
			transition -> M
			event m : Test?In
			guard m.c == '\'t\''
			
			transition -> L
		}
		state L {
			on entry TestOut('\'L\'')
			
			transition -> M
			event m : Test?In
			guard m.c == '\'t\''
			
			transition -> M
		}

		state M {
			on entry TestOut('\'M\'')
			
			transition -> M
			event m : Test?In
			guard m.c == '\'t\''
			
			internal event Test?Done
			action TestDone()
		}
	}
}
