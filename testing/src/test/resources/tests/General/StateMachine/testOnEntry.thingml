import "../../thingml.thingml"

thing TestOnEntry includes Test 
@test "t # E"
@test "tt # EE"
@test "tdfgt # EE"
@test "t* # E*"
{
	statechart TestOnEntry init I {
		internal event Test?Done
		action TestDone()
	
		state I {
			transition -> J
			event m : Test?In
			guard m.c == '\'t\''
		}
		state J {
			on entry TestOut('\'E\'')
			transition -> J
			event m : Test?In
			guard m.c == '\'t\''
		}
	}
}